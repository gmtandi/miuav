<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.view.percent.PercentRelativeLayout app:orientation="1" app:id="@id/activity_general_setting" app:layout_width="-1" app:layout_height="-1">
    <ImageButton app:id="@id/prev" app:visibility="2" style="@*style/MediaButton.Previous" />
    <ImageButton app:id="@id/rew" app:visibility="2" style="@*style/MediaButton.Rew" />
    <ImageButton app:id="@id/ffwd" app:visibility="2" style="@*style/MediaButton.Ffwd" />
    <ImageButton app:id="@id/next" app:visibility="2" style="@*style/MediaButton.Next" />
    <com.fimi.kernel.view.percent.PercentRelativeLayout app:id="@id/process_play" app:background="#cc1f1f21" app:layout_width="-1" app:layout_height="67dp">
        <TextView app:background="#26ffffff" app:layout_width="-1" app:layout_height="0.33dp" />
        <TextView app:textSize="12sp" app:textColor="#ffffffff" app:id="@id/time_current" app:layout_width="-2" app:layout_height="-2" app:layout_marginLeft="20dp" app:layout_marginTop="21dp" />
